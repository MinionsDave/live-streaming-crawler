{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "commonjs": true
    },
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "require-jsdoc": 0,
        "max-len": 0,
        "new-cap": 0,
        "arrow-parens": 0
    }
}
